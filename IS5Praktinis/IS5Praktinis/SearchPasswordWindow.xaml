<Window x:Class="IS5Praktinis.SearchPasswordWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:IS5Praktinis"
        mc:Ignorable="d"
        Title="SearchPasswordWindow" Height="450" Width="800">
    <Grid>
        <StackPanel Margin="20">
            <TextBlock Text="Enter Title to Search" />
            <TextBox Name="SearchTitleTextBox" />
            <Button Content="Search" Click="SearchButton_Click" Margin="0,20,0,0"/>
            <TextBlock Name="ResultTextBlock" TextWrapping="Wrap" Margin="0,20,0,0"/>
            <Button Name="ShowPasswordButton" Content="Show Password" Click="ShowPasswordButton_Click" Margin="0,10,0,0" Visibility="Collapsed"/>
        </StackPanel>
    </Grid>
</Window>
