<Window x:Class="IS5Praktinis.RegistrationWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:IS5Praktinis"
        mc:Ignorable="d"
        Title="RegistrationWindow" Height="450" Width="800">
    <Grid>
        <StackPanel Margin="20">
            <TextBlock Text="Username" />
            <TextBox Name="UsernameTextBox" />
            <TextBlock Text="Password" Margin="0,10,0,0"/>
            <PasswordBox Name="PasswordBox" />
            <Button Content="Register" Click="RegisterButton_Click" Margin="0,20,0,0"/>
        </StackPanel>
    </Grid>
</Window>
