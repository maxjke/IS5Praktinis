<Window x:Class="IS5Praktinis.DeletePasswordWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:IS5Praktinis"
        mc:Ignorable="d"
        Title="DeletePasswordWindow" Height="450" Width="800">
    <Grid>
        <StackPanel Margin="20">
            <TextBlock Text="Enter Title to Delete" />
            <TextBox Name="TitleTextBox" />
            <Button Content="Delete" Click="DeleteButton_Click" Margin="0,20,0,0"/>
        </StackPanel>
    </Grid>
</Window>
